{
    "name": "utxt",
    "displayName": "UTXT Doc",
    "description": "UTXT formatted text document",
    "version": "0.0.1",
    "publisher": "alxnull",
    "repository": {
        "type": "git",
        "url": "https://github.com/alxnull/vscode-smallbasic"
    },
    "icon": "assets/icon.png",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "utxt",
            "aliases": ["UTXT Doc", "utxt"],
            "extensions": [".txt", ".utxt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "utxt",
            "scopeName": "source.utxt",
            "path": "./syntaxes/utxt.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "utxt",
                "path": "./snippets/utxt.snippets.json"
            }
        ]
    }
}